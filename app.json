{
  "expo": {
    "name": "goit-react-native-hw",
    "slug": "goit-react-native-hw",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.goit-react-native-hw",
      "config": {
        "googleMapsApiKey": "EXPO_PUBLIC_IOS_GOOGLE_KWY"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA0SuB0vajcHvdHB27zez8BuYepf6kbQH0"
        }
      },
      "package": "com.anonymous.goitreactnativehw"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
